-- Toggle
pcall(function()game:WaitForChild("CoreGui"):FindFirstChild("Toggle"):Destroy()end)
g=Instance.new("ScreenGui",game:GetService("CoreGui"))
g.Name="Toggle"
g.ResetOnSpawn=false
t=Instance.new("TextButton",g)
t.Position=UDim2.new(0,2,0,80)
t.Size=UDim2.new(0,60,0,30)
t.Text="Open"
t.TextSize=24
t.TextColor3=Color3.fromRGB(40,40,40)
t.Font=Enum.Font.SourceSans
t.Active=true
t.Draggable=true
t.BackgroundTransparency=0.9
Instance.new("UICorner",t).CornerRadius=UDim.new(0,3)
s=Instance.new("UIStroke",t)
s.Color=Color3.new(1,1,1)
s.ApplyStrokeMode=Enum.ApplyStrokeMode.Border
t.MouseButton1Click:Connect(function()
Library:ToggleUI()
end)