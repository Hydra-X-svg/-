getgenv().Y=v
end)
Visual:TextBox("Z | Up/Down",function(v)
getgenv().Z=v
end)

Visual:Button("Block Reach",function()
local o=Instance.new("SelectionBox",tool.Handle)
o.Adornee=tool.Handle
o.Color3=Color3.new(0,0,0)
o.Transparency=0.9
tool.Handle.Shape="Block"
tool.Handle.Size=Vector3.new(X,Y,Z)
tool.Handle.Massless=true
end)

Visual:Button("Clear Reach",function()
tool.Handle.SelectionBox:Destroy()
end)

