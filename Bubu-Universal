local plr=game:GetService("Players")
local Lp=plr.LocalPlayer
local rs=game:GetService("RunService")
local map=game.MarketplaceService:GetProductInfo(game.PlaceId)
local function isNumber(str)
if tonumber(str)~=nil or str=='inf' then
 return true
 end
end

-- ESP [Billboard]
local espObjects={}
local function applyESP(character)
if character and character:FindFirstChild("HumanoidRootPart") then
 local board=Instance.new("BillboardGui",character)
 board.Size=UDim2.new(0,180,0,40)
 board.Adornee=character:FindFirstChild("Head")
 board.StudsOffset=Vector3.new(0,2,0)
 board.AlwaysOnTop=true
 local text=Instance.new("TextLabel",board)
 text.Size=UDim2.new(1,0,1,0)
 text.Text="Loading..."
 text.TextColor3=Color3.new(1,1,1)
 text.TextSize=14
 text.Font="GothamBold"
 text.TextStrokeTransparency=0.5
 text.BackgroundTransparency=1
 espObjects[character]={highlight,board,text}
 end
end

local function updateESP()
for _,plr in pairs(plr:GetPlayers()) do
 if plr~=Lp and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
  local chr=plr.Character
  local hrp=chr:FindFirstChild("HumanoidRootPart")
  local hum=chr:FindFirstChild("Humanoid")
  local espData=espObjects[chr]
  if hrp and hum and espData then
   local distance=(hrp.Position-Lp.Character.HumanoidRootPart.Position).Magnitude
   local health=math.floor(hum.Health)
   local maxHealth=math.floor(hum.MaxHealth)
   espData[3].Text=string.format("%.1fm | %d/%d HP | %s",distance,health,maxHealth,plr.DisplayName)
   end
  end
 end
end

local function refreshESP()
for _,data in pairs(espObjects) do
 for _,obj in pairs(data) do
  obj:Destroy()
 end
end
espObjects={}
for _,plr in pairs(plr:GetPlayers()) do
 if plr~=Lp and plr.Character then
  applyESP(plr.Character)
  end
 end
end

getgenv().HitboxSize=10
getgenv().Hitbox=false
getgenv().Ws=Lp.Character.Humanoid.WalkSpeed
getgenv().Jp=Lp.Character.Humanoid.JumpPower
getgenv().TPSpeed=1
getgenv().TPWalk=false

