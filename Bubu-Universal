Visual:InfoLabel("some script might be error!")
Visual:Section("Visuals")
Visual:TextBox("Hitbox Size",function(v)
getgenv().HitboxSize=v
end)

Visual:Toggle("Hitbox",function(s)
getgenv().Hitbox=s
rs.RenderStepped:connect(function()
if Hitbox==true then
 for i,v in next,plr:GetPlayers() do
  if v.Name~=Lp.Name then
   pcall(function()
    v.Character.HumanoidRootPart.Size=Vector3.new(HitboxSize,HitboxSize,HitboxSize)
    v.Character.HumanoidRootPart.Transparency=0.9
    v.Character.HumanoidRootPart.BrickColor=BrickColor.new("Really black")
    v.Character.HumanoidRootPart.Material="Neon"
    v.Character.HumanoidRootPart.CanCollide=false
   end)
  end
 end
else
 for i,v in next,plr:GetPlayers() do
  if v.Name~=Lp.Name then
   pcall(function()
    v.Character.HumanoidRootPart.Size=Vector3.new(2,2,1)
    v.Character.HumanoidRootPart.Transparency=1
    v.Character.HumanoidRootPart.BrickColor=BrickColor.new("Medium stone grey")
    v.Character.HumanoidRootPart.Material="Plastic"
    v.Character.HumanoidRootPart.CanCollide=false
    end)
   end
  end
 end
end)
end)

Visual:Toggle("Highlight",function(s)
getgenv().enabled=s
loadstring(game:HttpGet("https://pastefy.app/6G2dvWJD/raw"))();
end)

Visual:Toggle("ESP [Billboard]",function(s)
getgenv().EspBillboard=s
if EspBillboard then
 rs.RenderStepped:Connect(function()
  if EspBillboard then
   refreshESP()
   updateESP()
  end
 end)
else
for _,data in pairs(espObjects) do
 for _,obj in pairs(data) do
  obj:Destroy()
  end
 end
 espObjects={}
end
end)

Visual:Section("LocalPlayer")
Visual:TextBox("WalkSpeed",function(v)
getgenv().Ws=v
pcall(function()
 Lp.Character.Humanoid.WalkSpeed=v
end)
end)

Visual:Toggle("Loop WalkSpeed",function(s)
getgenv().loopW=s
rs.Heartbeat:Connect(function()
if loopW==true then
 pcall(function()
  Lp.Character.Humanoid.WalkSpeed=Ws
  end)
 end
end)
end)

Visual:TextBox("JumpPower",function(v)
getgenv().Jp=v
pcall(function()
 Lp.Character.Humanoid.JumpPower=v
end)
end)

Visual:Toggle("Loop JumpPower",function(s)
getgenv().loopJ=s
rs.Heartbeat:Connect(function()
if loopJ==true then
 pcall(function()
  Lp.Character.Humanoid.JumpPower=Ws
  end)
 end
end)
end)

Visual:TextBox("TP-Speed",function(v)
getgenv().TPSpeed=v
end)

Visual:Toggle("TP-Walk",function(s)
getgenv().TPWalk=s
local chr=Lp.Character
local hb=rs.Heartbeat
local hum=chr and chr:FindFirstChildWhichIsA("Humanoid")
while getgenv().TPWalk and hb:Wait() and chr and hum and hum.Parent do
 if hum.MoveDirection.Magnitude>0 then
  if getgenv().TPSpeed and isNumber(getgenv().TPSpeed) then
   Lp.Character:TranslateBy(hum.MoveDirection*tonumber(getgenv().TPSpeed))
 else
   Lp.Character:TranslateBy(hum.MoveDirection)
  end
 end
end
end)

Visual:Toggle("Noclip",function(s)
getgenv().Noclip=s
if Noclip then
 rs.Heartbeat:Connect(function()
 if Noclip then
  Lp.Character.Head.CanCollide=false
  Lp.Character.Torso.CanCollide=false
else
  Lp.Character.Head.CanCollide=true
  Lp.Character.Torso.CanCollide=true
  end
 end)
end
end)

Visual:Toggle("Inf Jump",function(s)
getgenv().InfJ=s
game:GetService("UserInputService").JumpRequest:connect(function()
if InfJ==true then
  Lp.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
 end
end)
end)

Visual:Slider("Fov",70,120,function(v)
game.Workspace.CurrentCamera.FieldOfView=v
end)

Visual:Slider("Brightness",0,3,function(v)
game.Lighting.ExposureCompensation=v
end)

