Player:Toggle("Loop WalkSpeed",function(s)
getgenv().loopW=s
rs.Heartbeat:Connect(function()
 if loopW==true then
  pcall(function()
   Lp.Character.Humanoid.WalkSpeed=Ws
   end)
  end
 end)
end)

Player:TextBox("JumpPower",function(v)
getgenv().Jp=v
