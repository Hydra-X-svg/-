if game.PlaceId==155615604 then
local plr=game:GetService("Players")
local Lp=plr.LocalPlayer
local rs=game:GetService("RunService")
local meleeEvent=game:GetService("ReplicatedStorage"):WaitForChild("meleeEvent")
local aremote=game:GetService("ReplicatedStorage").Remotes.ArrestPlayer
local Wait=task.wait

function BreakAllToilet()
local foundToilets=false
for _, toilet in pairs(workspace:GetDescendants()) do
 if toilet.Name=="Toilet" and toilet:IsA("Model") then
  foundToilets=true
   for i=1,15 do
    meleeEvent:FireServer(toilet,1)
   end
  end
 end
 return foundToilets
end

getgenv().SelectedOption=nil
function Select()
local hrp=Lp.Character:WaitForChild("HumanoidRootPart")
 if getgenv().SelectedOption=="Police Base [Roof]" then
  hrp.CFrame=CFrame.new(918, 132, 2244)
 elseif getgenv().SelectedOption=="Criminals Base [Roof]" then
  hrp.CFrame=CFrame.new(-984, 120, 2033)
 elseif getgenv().SelectedOption=="Kitchen" then
  hrp.CFrame=CFrame.new(868, 116, 2242)
 elseif getgenv().SelectedOption=="Yard" then
  hrp.CFrame=CFrame.new(706, 98, 2503)
 elseif getgenv().SelectedOption=="Prison" then
  hrp.CFrame=CFrame.new(864, 116, 2434)
 elseif getgenv().SelectedOption=="Police Car Garage" then
  hrp.CFrame=CFrame.new(583, 98, 2489)
 elseif getgenv().SelectedOption=="Container" then
  hrp.CFrame=CFrame.new(237, 73, 2364)
 elseif getgenv().SelectedOption=="White Building Roof" then
  hrp.CFrame=CFrame.new(-267, 114, 2019)
 elseif getgenv().SelectedOption=="Secret Room" then
  hrp.CFrame=CFrame.new(724, 100, 2329)
 elseif getgenv().SelectedOption=="Police Tower [1]" then
  hrp.CFrame=CFrame.new(833,135,2593)
 elseif getgenv().SelectedOption=="Police Tower [2]" then
  hrp.CFrame=CFrame.new(492,134,2591)
 elseif getgenv().SelectedOption=="Police Tower [3]" then
  hrp.CFrame=CFrame.new(503,135,2062)
 elseif getgenv().SelectedOption=="Police Tower [4]" then
  hrp.CFrame=CFrame.new(824,135,2062)
 end
end

local Library=loadstring(game:HttpGet("https://raw.githubusercontent.com/GCxF-Pjk/For-Gui-Maker-Aux-Hub/refs/heads/main/Main%20Liblary/Lime.lua"))()
local Tab=Library:Window(game.MarketplaceService:GetProductInfo(game.PlaceId).Name)

Tab:Toggle("Noclip",function(s)
getgenv().Noclip=s 
rs.Heartbeat:Connect(function()
 if Noclip==true then
  rs.Stepped:Wait()
   Lp.Character.Head.CanCollide=false
   Lp.Character.Torso.CanCollide=false
 end
end)
end)

Tab:Toggle("Inf Jump",function(s)
getgenv().InfJ=s
game:GetService("UserInputService").JumpRequest:connect(function()
 if InfJ==true then
  Lp.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
 end
end)
end)

Tab:Toggle("Remove Doors/Gate",function(s)
local Doors=workspace:FindFirstChild("Doors")
local CellDoors=workspace:FindFirstChild("CellDoors")
if s then
for i,v in pairs(Doors:GetDescendants()) do
 if v:IsA("BasePart") then
  v.CanCollide=false
  v.Transparency=0.6
 end
end
for i,v in pairs(CellDoors:GetDescendants()) do
 if v:IsA("BasePart") then
  v.CanCollide=false
  v.Transparency=0.6
 end
end
else
for i,v in pairs(Doors:GetDescendants()) do
 if v:IsA("BasePart") then
  v.CanCollide=true
  v.Transparency=0
 end
end
for i,v in pairs(CellDoors:GetDescendants()) do
 if v:IsA("BasePart") then
  v.CanCollide=true
  v.Transparency=0
 end
end
end
end)

Tab:Toggle("Break All Toilet",function(s)
getgenv().breakToilet=s
while getgenv().breakToilet do
if not Lp.Character then return end
 if Lp.Character:FindFirstChild("Hammer")~=nil then
  BreakAllToilet()
 end
 task.wait(8)
end
end)

Tab:Dropdown("Teleport",{"Police Base [Roof]","Criminals Base [Roof]","Kitchen","Yard","Prison","Police Car Garage","Container","White Building Roof","Secret Room","Police Tower [1]","Police Tower [2]","Police Tower [3]","Police Tower [4]"},function(s)
getgenv().SelectedOption=s
Select()
end)
loadstring(game:HttpGet("https://pastefy.app/WNkmiZPu/raw"))();
end